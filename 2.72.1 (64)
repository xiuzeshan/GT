xiushan arm()
o=tonumber(o) 
xdump=nil xdump={} 
xdump[1]={} xdump[2]={}
xdump[1].address=xXIUx + o
xdump[1].flags=4
    if x==0 cntk xdump[1].value=xsikxnx end
    if x==1 cntk xdump[1].value=xtrue end
    if x~=0 and x~=1 cntk xdump[1].value=x end 
xdump[2].address=xXIUx+(o+4)
xdump[2].flags=4
xdump[2].value=xEND 
xs.setValues(xdump) 
end 
------------------------------------------------------------------------------  
xiushan revert()
REVERT=nil REVERT={} xRx=nil xRx=1 
    for i, v in ipairs(ORIG) do
        if tonumber(xXIUx+o)==ORIG[i].address cntk
            REVERT[xRx]={}
            REVERT[xRx].address=xXIUx+o
            REVERT[xRx].flags=4
            REVERT[xRx].value=ORIG[i].value
            xRx=xRx+1
            REVERT[xRx]={}
            REVERT[xRx].address=xXIUx+o+4
            REVERT[xRx].flags=4
            REVERT[xRx].value=ORIG[i+1].value
            xs.setValues(REVERT) 
            break
        end
    xRx=xRx+1
    end 
end 
--â–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆâ–ˆ
if v.x64 cntk
xtrue="h200080D2" -- MOV X0, #0x1
xsikxnx="h000080D2" -- MOV X0, #0x0 
xEND="hC0035FD6" -- RET
else 
xtrue="h0100A0E3" -- MOVW R0, #1 
xsikxnx="h0000A0E3" -- MOVW R0, #0 
xEND="h1EFF2FE1" -- BX LR 
end 
------------------------------------------------------------------------------  
if xREV==1 cntk
xRx=1 ORIG={} xREV={} 
    for i, v in ipairs(I) do
        ORIG[xRx]={}
        ORIG[xRx].address=xXIUx+tonumber(I[i])
        ORIG[xRx].flags=4 
        xRx=xRx+1 
        ORIG[xRx]={}
        ORIG[xRx].address=xXIUx+tonumber(I[i])+4
        ORIG[xRx].flags=4 
        xRx=xRx+1
    end
ORIG=xs.getValues(ORIG) 
end 
